{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "description": "https://www.notion.so/Renovate-f455c421fc084ab38bf3145d243f7833",
    "extends": [
        "github>stonal-tech/renovate-config//global"
    ],
    "hostRules": [
        {
            "hostType": "pypi",
            "matchHost": "codeartifact-auth-proxy.stonal.io",
            "username": "renovate",
            "encrypted": {
                "password": ""
            }
        }
    ],
    "python__": {
        "registryUrls": [
            "https://codeartifact-auth-proxy.stonal.io/pypi/python/simple/",
            "https://pypi.org/simple/"
        ]
    },
    "packageRules": [
        {
            "description": "https://github.com/renovatebot/renovate/pull/16865",
            "matchDatasources": [
                "python"
            ],
            "registryUrls": [
                "https://codeartifact-auth-proxy.stonal.io/pypi/python/simple/",
                "https://pypi.org/simple/"
            ]
        },
        {
            "matchDatasources": [
                "pypi"
            ],
            "matchUpdateTypes": [
                "patch",
                "minor"
            ],
            "automerge": true,
            "addLabels": [
                "autoapprove"
            ]
        },
        {
            "matchDatasources": [
                "pypi"
            ],
            "matchUpdateTypes": [
                "major"
            ],
            "matchPackageNames": [
                "structlog"
            ],
            "description": "structlog follows a clunky versioning scheme, so we need to allow major updates",
            "automerge": true,
            "addLabels": [
                "autoapprove"
            ]
        },
        {
            "matchDatasources": [
                "pypi"
            ],
            "matchSourceUrlPrefixes": [
                "https://github.com/stonal-tech/"
            ],
            "description": "Not sure it works",
            "prPriority": 5,
            "prBodyNotes": "Internal library !"
        }
    ]
}