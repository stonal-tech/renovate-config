{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "regex": {
        "enabled": true
    },
    "regexManagers": [
        {
            "fileMatch": [".*\\.html?$", ".*\\.php$"],
            "matchStrings": [
                "https://cdn.stonal.io/builds/(?<depName>[^\/]+)/(?<currentValue>[^\/])/.*"
            ],
            "datasourceTemplate": "github-tags",
            "depNameTemplate": "stonal-tech/{{{depName}}}"
        }
    ]
}
