{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "hostRules": [
        {
            "hostType": "maven",
            "matchHost": "https://codeartifact-auth-proxy.stonal.io/",
            "username": "renovate",
            "encrypted": {
                "password": ""
            }
        }
    ],
    "packageRules": [
        {
            "matchDatasources": [
                "maven"
            ],
            "registryUrls": [
                "https://codeartifact-auth-proxy.stonal.io/maven/avatar/",
                "https://repo.maven.apache.org/maven2/",
                "https://packages.confluent.io/maven/"
            ]
        }
    ]
}