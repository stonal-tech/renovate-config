{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "hostRules": [
        {
            "hostType": "npm",
            "matchHost": "https://aws-auth-proxy.stonal.io/codeartifact/npm/avatar/",
            "username": "renovate",
            "encrypted": {
                "password": ""
            }
        },
        {
            "hostType": "npm",
            "matchHost": "https://npm.pkg.github.com/",
            "username": "stonalbot",
            "encrypted": {
                "password": ""
            }
        }
    ],
    "npmrc": "@lfn:registry=https://aws-auth-proxy.stonal.io/codeartifact/npm/avatar/\n@stonal-tech:registry=https://npm.pkg.github.com/",
    "packageRules": [
        {
            "matchDatasources": [
                "npm"
            ],
            "matchPackageNames": [
                "^@lfn/.*",
                "^@stonal/.*"
            ],
            "registryUrls": [
                "https://aws-auth-proxy.stonal.io/codeartifact/npm/avatar/"
            ]
        },
        {
            "matchDatasources": [
                "npm"
            ],
            "matchPackagePatterns": [
                "^@stonal-tech/.*"
            ],
            "registryUrls": [
                "https://npm.pkg.github.com/"
            ]
        },
        {
            "matchDatasources": [
                "npm"
            ],
            "matchPackagePatterns": [
                "^@types/jasmine",
                "^jasmine"
            ],
            "groupName": "jasmine"
        }
    ]
}
